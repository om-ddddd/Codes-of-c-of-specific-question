#include <bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n];
        stack<int> s;
        cin>>a[0];
        cout<<0<<" ";
        int min=a[0];
        int min_pos=0;
        for(int i=1;i<n;i++){
            cin>>a[i];
            if(min>a[i])
           { min_pos=i;
             min=a[i];
             cout<<"0 ";
           }
           else if(min==a[i])
           {
            min_pos=i;
            cout<<"0 ";
           }
           else cout<<min_pos+1<<" ";

        }
    cout<<'\n';
    }
}
